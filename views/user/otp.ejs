<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
		<link rel="icon" type="image/png" href="/user/images/logo-removebg-preview.png" sizes="32x32"/>
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="/user/fonts/font.css">

    <link rel="stylesheet" href="/user/css/owl.carousel.min.css">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="/user/css/bootstrap.min.css">
    
    <!-- Style -->
    <link rel="stylesheet" href="/user/css/login.css">

    <title>Login-woodQ</title>
  </head>
  <body>
  
      <div class="container">
        <div class="row align-items-center justify-content-center">
          <div class="col-md-7">
            <h3>Login to <strong>WoodQ</strong></h3>
            <p class="mb-4">Enter OTP Verification .</p>
            <form action="/otp" method="post">
              <div class="form-group first">
                <label for="username">Enter OTP</label>
                <input type="text" class="form-control"  value="<%=data.username%>"  name="username" hidden >
                <input type="number" class="form-control" placeholder="Enter OTP"   name="otp"  >
                <p style="color: rgb(255, 35, 35);" ><%=locals.wrong%></p>
                
                <input type="email" class="form-control"  value="<%=data.email%>"  name="email" hidden >
            
                <input type="number" class="form-control"   value="<%=data.phone%>"  name="phone" hidden >
            
                <input type="password" class="form-control"  value="<%=data.password%>"   name="password" hidden >
            
            
              
              <input type="submit" value="submit" class="btn btn-block btn-primary">
              

            </form>
          </div>
        </div>
      </div>
    </div>
    <script src="js/bootstrap.min.js"></script>
    
  </body>
</html>